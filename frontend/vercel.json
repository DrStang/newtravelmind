{\n  \"builds\": [\n    {\n      \"src\": \"package.json\",\n      \"use\": \"@vercel/static-build\",\n      \"config\": {\n        \"buildCommand\": \"npm run build\",\n        \"distDir\": \"dist\"\n      }\n    }\n  ],\n  \"routes\": [\n    {\n      \"src\": \"/assets/(.*)\",\n      \"headers\": {\n        \"cache-control\": \"public, max-age=31536000, immutable\"\n      }\n    },\n    {\n      \"src\": \"/(.*)\",\n      \"dest\": \"/index.html\"\n    }\n  ],\n  \"env\": {\n    \"VITE_API_BASE_URL\": \"@api_base_url\",\n    \"VITE_WS_URL\": \"@ws_url\",\n    \"VITE_GOOGLE_MAPS_API_KEY\": \"@google_maps_api_key\"\n  }\n}